[run]

branch = True

source =
    hookah/
    tests/

[report]

skip_empty = True
show_missing = True
sort = -cover

exclude_lines =
    def __repr__
    def __str__

    raise AssertionError
    raise NotImplementedError

    if 0:
    if False:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    assert False

    pragma: unreachable
    pragma: debugging
